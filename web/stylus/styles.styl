@import "reset"

body
	font-family Verdana, sans-serif
	font-weight normal
	font-size 12px

header
	margin-bottom 5px
	padding 10px
	background-color #888
	h1
		display inline
		color blue
		text-align left
	ul
		display inline-block
		width 100%
		text-align right
		li
			display inline-block

#map 
	width 100%
	height 80%


#enc_list 
	padding 10px
	text-align center
	margin-top 5px
	background-color #888


#helper_div
	display none
	position fixed
	top 150px
	right 50px
	background-color white
	width 30%
	padding 15px

